{
    "cmake.sourceDirectory": "${workspaceFolder}",
    "cmake.buildDirectory": "${workspaceFolder}/../klee_build",
    "cmake.configureArgs": [
        "-DLLVM_DIR=${env:HOME}/.local/llvm-13/",
        "-DLLVMCC=${env:HOME}/.local/llvm-13/bin/clang",
        "-DLLVMCXX=${env:HOME}/.local/llvm-13/bin/clang++",
        "-DENABLE_SOLVER_Z3=ON",
        "-DENABLE_POSIX_RUNTIME=ON",
        "-DKLEE_UCLIBC_PATH=${env:HOME}/klee/klee_deps/klee-uclibc/",
        "-DENABLE_UNIT_TESTS=ON",
        "-DGTEST_SRC_DIR=${env:HOME}/klee/klee_deps/googletest-release-1.11.0",
        "-DENABLE_KLEE_LIBCXX=ON",
        "-DENABLE_LIBCXX_DIR=${env:HOME}/klee/klee_deps/libcxx/libc++-install-130/",
        "-DKLEE_LIBCXX_INCLUDE_DIR=${env:HOME}/klee/klee_deps/libcxx/libc++-install-130/include/c++/v1",
        "-DKLEE_LIBCXX_BC_PATH=${env:HOME}/klee/klee_deps/libcxx/libc++-install-130/lib",
        "-DENABLE_KLEE_EH_CXX=ON",
        "-DKLEE_LIBCXXABI_SRC_DIR=${env:HOME}/llvm-project/libcxxabi",
        "-DENABLE_TCMALLOC=ON",
        "-DCMAKE_BUILD_TYPE=Debug",
        "-DCMAKE_EXPORT_COMPILE_COMMANDS=1"
    ],
    "cmake.buildArgs": [
        "-j256"
    ]
}
